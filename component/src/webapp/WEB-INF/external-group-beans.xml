<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<bean id="uk.ac.ox.oucs.vle.ExternalGroupManager"
		class="uk.ac.ox.oucs.vle.ExternalGroupManagerImpl">
		<!-- If you have explicity defined a connection manager user this
		<property name="ldapConnectionManager">
			<ref bean="edu.amc.sakai.user.LdapConnectionManager"/>
		</property>
		  -->
		<property name="jldapDirectoryProvider">
			<ref bean="edu.amc.sakai.user.JLDAPDirectoryProvider"/>
		</property>
		<property name="userDirectoryService">
			<ref bean="org.sakaiproject.user.api.UserDirectoryService"/>
		</property>
	</bean>
</beans>